{
    "name": "roublez/isin-collector",
    "description": "A command-line interface to download, parse and format ISIN datasets.",
    "keywords": [
        "isin", "downloader", "parser", "formatter"
    ],
    "homepage": "https://github.com/roublez/isin-collector",
    "license": "MIT",
    "authors": [
        {
            "name": "Dennis Prudlo",
            "email": "mail@dennisprudlo.com",
            "homepage": "https://dennisprudlo.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/roublez/isin-collector/issues"
    },
    "require": {
        "php": ">=8.0",
        "symfony/console": "^5.3",
        "nunomaduro/collision": "^5.5",
        "symfony/yaml": "^5.3",
        "illuminate/collections": "^8.51",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/support": "^8.54"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.3"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Roublez\\Isin\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    }
}
